{
  "manifest_version": 2,
  "name": "Play list",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [ "https://vk.com/*" ],
      "js": [ "jquery-3.1.1.min.js", "content.js" ],
      "run_at": "document_end",
      "css": [ "styles.css" ]
    }
  ],
  "permissions": [
    "https://vk.com/",
    "http://kriperplaylist.azurewebsites.net/"
  ],

  "browser_action": {
    "default_title": "PlayList"
  },
  "web_accessible_resources": [ "sync2.png" ]

}